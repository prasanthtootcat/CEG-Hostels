<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tootcat.ceghostel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Registeration" />
        <activity android:name=".Announcement" />
        <activity android:name=".FeePayment" />
        <activity android:name=".Receipt" />
        <activity android:name=".Menu" />
        <activity android:name=".Feedback" />
        <activity android:name=".MenuPreference"></activity>
    </application>

</manifest>